	<queryString language="mdx">
		<![CDATA[
WITH MEMBER Measures.[Net Amounts] AS
[Measures].[Extended Amount] - [Measures].[Received Amount]
SELECT
NON EMPTY {[Measures].[Extended Amount], Measures.[Net Amounts]} ON COLUMNS,
NON EMPTY {Hierarchize({[Type of Flow].[Name].Members})} ON ROWS
FROM [Financial]
WHERE {[Type of Finance].[Name].[GRANT], [Type of Finance].[Name].[Aid grant excluding debt reorganisation], [Type of Finance].[Name].[Subsidies to national private investors]}
]]>
	</queryString>
