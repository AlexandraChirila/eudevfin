<ehcache>
	<cache name="pentaho-cda-dataaccess"
        maxElementsInMemory="2000"
        eternal="false"
        overflowToDisk="true"
        timeToIdleSeconds="0"
        timeToLiveSeconds="0"
        diskPersistent="false"
        diskExpiryThreadIntervalSeconds="360"
        memoryStoreEvictionPolicy="LFU"
        diskSpoolBufferSizeMB="50">
	</cache>
</ehcache>
