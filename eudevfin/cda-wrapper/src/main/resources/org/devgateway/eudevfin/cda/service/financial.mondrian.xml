      </Level>
    </Hierarchy>
  </Dimension>
  <Dimension visible="true" highCardinality="false" name="Type of Aid">
    <Hierarchy visible="true" hasAll="true" allMemberName="All Types of Aid" allMemberCaption="All Types of Aid" primaryKey="ID" caption="Type of Aid">
      <Table name="CATEGORY" schema="APP">
      </Table>
      <Level name="code" visible="true" table="CATEGORY" column="CODE" type="String" uniqueMembers="false" levelType="Regular" hideMemberIf="Never">
