<?xml version="1.0" encoding="UTF-8"?>
<enunciate xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://enunciate.codehaus.org/schemas/enunciate-1.24.xsd">

	<api-classes>
		<include pattern="org.saiku.web.rest.**" />
	</api-classes>
	<api-import pattern="org.saiku.web.rest.**" />

	<services>
		<rest>
			<custom-resource-parameter-annotation
				qualifiedName="com.sun.jersey.multipart.FormDataParam" />
		</rest>
	</services>

	<webapp mergeWebXML="src/main/web/web.xml" />

	<modules>

		<spring-app>
			<springImport file="src/main/web/applicationContext.xml" />
			<springImport file="src/main/web/saiku-beans.xml" />
			<springImport file="src/main/web/spring-servlet.xml" />
		</spring-app>
		<jersey useSubcontext="true" disableWildcardServletError="false"
			disabled="true">
			<init-param name="com.sun.jersey.config.property.packages"
				value="org.saiku.web" />
			<init-param name="com.sun.jersey.spi.container.ContainerRequestFilters"
				value="com.sun.jersey.api.container.filter.PostReplaceFilter" />
		</jersey>
		<jaxws disabled="true" />
		<jaxws-ri disabled="true" forceSpringEnabled="true" />
		<cxf disabled="true" />
		<amf disabled="true" />
		<c disabled="true" forceEnable="true" />
		<csharp disabled="true" />
		<gwt disabled="true" />
		<jaxws-client disabled="true" />
		<obj-c disabled="true" forceEnable="true" />
		<rest disabled="false" />
		<ruby disabled="false" forceEnable="true" />
		<xfire disabled="true" />
		<xfire-client disabled="true" />
		<xml disabled="true" />
	</modules>



</enunciate>
